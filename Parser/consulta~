SELECT AVG(ga_hib_db), ga_cluster_i, gap_i, gap_tt, gap_pc, gap_pm FROM genetico, geneticop WHERE ga_type = gap_id and ga_cluster_f = 5 GROUP BY ga_type ORDER BY AVG(ga_hib_db) ASC LIMIT 20;
